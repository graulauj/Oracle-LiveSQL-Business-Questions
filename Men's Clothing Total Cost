SELECT PRODUCT_NAME, 
SUM(products.UNIT_PRICE) 
TOT_COST FROM CO."ORDERS" orders LEFT JOIN CO."ORDER_ITEMS" items ON orders.ORDER_ID = items.ORDER_ID LEFT JOIN CO."PRODUCTS" products  ON items.PRODUCT_ID = products.PRODUCT_ID 
WHERE ORDER_STATUS = 'COMPLETE' 
GROUP BY PRODUCT_NAME HAVING PRODUCT_NAME LIKE 'Men%'
